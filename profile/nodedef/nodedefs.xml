<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller, this must mach the id in your Controller class-->
    <nodeDef id="hdctrl" nls="ctl">
        <editors />
        <sts>
		<st id="ST" editor="bool" />
	</sts>
        <cmds>
            <sends>
              <!-- These are for our heartbeat -->
              <cmd id="DON" />
              <cmd id="DOF" />
            </sends>
            <accepts>
              <cmd id="QUERY" />
              <cmd id="DISCOVER" />
              <cmd id="REMOVE_NOTICES_ALL" />
            </accepts>
        </cmds>
    </nodeDef>
	<!-- shadeid -->
    <nodeDef id="shadeid" nls="shade">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
            <st id="GV0" editor="id" />
            <st id="GV3" editor="shadecap" />
            <st id="GV5" editor="batteryst" />
            <st id="GV6" editor="id" />
            <st id="GV7" editor="position" />
            <st id="GV8" editor="position" />
            <st id="GV9" editor="tilt" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="OPEN" />
                <cmd id="CLOSE" />
                <cmd id="STOP" />
                <cmd id="TILTOPEN" />
                <cmd id="TILTCLOSE" />
                <cmd id="JOG" />
                <cmd id="QUERY" />
            </accepts>
        </cmds>
    </nodeDef>
	<!-- sceneid -->
    <nodeDef id="sceneid" nls="scene">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
            <st id="GV0" editor="id" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="ACTIVATE" />
                <cmd id="QUERY" />
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
