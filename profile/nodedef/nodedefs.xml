<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller, this must mach the id in your Controller class-->
    <nodeDef id="hdctrl" nls="ctl">
        <editors />
        <sts>
		<st id="ST" editor="bool" />
	</sts>
        <cmds>
            <sends>
              <!-- These are for our heartbeat -->
              <cmd id="DON" />
              <cmd id="DOF" />
            </sends>
            <accepts>
              <cmd id="QUERY" />
              <cmd id="DISCOVER" />
              <cmd id="REMOVE_NOTICES_ALL" />
            </accepts>
        </cmds>
    </nodeDef>
	<!-- nodeid -->
    <nodeDef id="nodeid" nls="node">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
                <cmd id="PING" />
            </accepts>
        </cmds>
    </nodeDef>
	<!-- shadeid -->
    <nodeDef id="shadeid" nls="shade">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="PING" />
            </accepts>
        </cmds>
    </nodeDef>
	<!-- sceneid -->
    <nodeDef id="sceneid" nls="scene">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="ACTIVATE" />
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
