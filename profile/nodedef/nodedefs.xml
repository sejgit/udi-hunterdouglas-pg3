<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller, this must mach the id in your Controller class-->
    <nodeDef id="hdctrl" nls="ctl">
        <editors />
        <sts>
		<st id="ST" editor="bool" />
	</sts>
        <cmds>
            <sends>
              <!-- These are for our heartbeat -->
              <cmd id="DON" />
              <cmd id="DOF" />
            </sends>
            <accepts>
              <cmd id="QUERY" />
              <cmd id="DISCOVER" />
              <cmd id="REMOVE_NOTICES_ALL" />
            </accepts>
        </cmds>
    </nodeDef>
	<!-- shadeid -->
    <nodeDef id="shadeid" nls="shade">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
            <st id="GV0" editor="id" />
            <st id="GV1" editor="id" />
            <st id="GV2" editor="position" />
            <st id="GV3" editor="position" />
            <st id="GV4" editor="tilt" />
            <st id="GV5" editor="shadecap" />
            <st id="GV6" editor="batteryst" />
        </sts>
        <cmds>
          <sends>
            <cmd id="OPEN" />
            <cmd id="CLOSE" />
            <cmd id="STOP" />
            <cmd id="TILTOPEN" />
            <cmd id="TILTCLOSE" />
            <cmd id="JOG" />
          </sends>
          <accepts>
            <cmd id="OPEN" />
            <cmd id="CLOSE" />
            <cmd id="STOP" />
            <cmd id="TILTOPEN" />
            <cmd id="TILTCLOSE" />
            <cmd id="JOG" />
            <cmd id="QUERY" />
            <cmd id="SETPOS" >
              <p id="SETPRIM" editor="position" optional="T" init='GV7'/>
              <p id="SETSECO" editor="position" optional="T" init='GV8'/>
              <p id="SETTILT" editor="tilt" optional="T" init='GV9'/>
            </cmd>
          </accepts>
        </cmds>
    </nodeDef>
	<!-- sceneid -->
    <nodeDef id="sceneid" nls="scene">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
            <st id="GV0" editor="id" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="ACTIVATE" />
                <cmd id="QUERY" />
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
